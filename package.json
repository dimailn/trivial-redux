{
  "name": "trivial-redux",
  "version": "2.0.0-alpha.2",
  "description": "",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "",
  "repository": "https://github.com/dimailn/trivial-redux",
  "scripts": {
    "test": "jest",
    "cov": "jest --coverage",
    "build": "tsc ./src/index.ts --outDir lib/ -d",
    "pub": "npm run build && npm publish"
  },
  "license": "ISC",
  "devDependencies": {
    "axios": "^0.27.2",
    "babel-jest": "^28.1.0",
    "coffee-script": "^1.12.6",
    "coveralls": "^3.0.2",
    "jest": "^28.1.0",
    "lodash.clonedeep": "^4.5.0",
    "ts-jest": "^28.0.2"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "exclude": [
          "**/__mocks__/*"
        ],
        "diagnostics": {
          "exclude": [
            "**/*.ts",
            "**/*.test.ts"
          ]
        },
        "tsconfig": {
          "isolatedModules": true
        }
      }
    },
    "moduleFileExtensions": [
      "js",
      "coffee",
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest",
      "^.+\\.js$": "babel-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ]
  },
  "dependencies": {
    "immer": "^9.0.2",
    "lodash": "^4.17.10"
  }
}
